{
    "name": "ergonext",
    "displayName": "Ergon Ext",
    "description": "Ajuda a navegar pelos arquivos de log e debug do sistema de RH Ergon (da Techne)",
    "version": "0.1.2",
    "publisher": "ViniciusPJardim",
    "repository": {
        "type": "git",
        "url": "https://github.com/viniciuspjardim/ergon-ext.git"
    },
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.carregarArquivos"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.carregarArquivos",
                "title": "Ergon: Carregar arquivos"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.70",
        "tslint": "^5.11.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.21"
    },
    "dependencies": {
        "iconv-lite": "^0.4.24"
    }
}
